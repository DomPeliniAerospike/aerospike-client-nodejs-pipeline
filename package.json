{
  "name": "aerospike",
  "version": "4.0.0",
  "description": "Aerospike Client Library",
  "keywords": [
    "aerospike",
    "database",
    "nosql"
  ],
  "homepage": "https://github.com/aerospike/aerospike-client-nodejs",
  "license": "Apache-2.0",
  "main": "./build/Release/aerospike",
  "engines": {
    "node": ">=4"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aerospike/aerospike-client-nodejs"
  },
  "bugs": {
    "url": "https://github.com/aerospike/aerospike-client-nodejs/issues"
  },
  "binary": {
    "module_name": "aerospike",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://www.npmjs.com/package/aerospike"
  },
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "preinstall": "npm install node-pre-gyp",
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "mocha --exit",
    "test-noserver": "GLOBAL_CLIENT=false mocha -g '#noserver'",
    "lint": "standard",
    "posttest": "npm run lint",
    "coverage": "nyc npm test && nyc --reporter=lcov report",
    "codecov": "codecov",
    "prepush": "npm test",
    "apidocs": "jsdoc -c jsdoc.json",
    "cppcheck": "cppcheck --quiet --enable=warning,style -I src/include src/main/",
    "valgrind": "valgrind node ./node_modules/mocha/bin/_mocha -R dot -g '#slow' -i",
    "prepare": "husky install",
    "__comment__": "prepare: husky install"
  },
  "dependencies": {
    "bindings": "^1.3.0",
    "minimatch": "^3.0.4",
    "nan": "^2.14.0",
    "node-gyp": "^8.4.1",
    "node-pre-gyp": "^0.17.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "choma": "^1.2.1",
    "codecov": "^3.8.3",
    "deep-eql": "^4.0.0",
    "dirty-chai": "^2.0.1",
    "husky": "^7.0.4",
    "jsdoc": "^3.6.10",
    "marked": "^4.0.10",
    "mocha": "^9.2.0",
    "mocha-clean": "^1.0.0",
    "nyc": "^15.0.0",
    "p-throttle": "^3.1.0",
    "semver": "^7.1.1",
    "standard": "^16.0.4",
    "tmp": "^0.1",
    "yargs": "^15.3.1"
  },
  "standard": {
    "ignore": [
      "apidocs",
      "tmp-*.js",
      "/*.js"
    ]
  },
  "files": [
    "aerospike-client-c.ini",
    "aerospike-client-c.sha256",
    "binding.gyp",
    "benchmarks/",
    "examples/",
    "lib/",
    "modules/lua-core/",
    "scripts/",
    "src/",
    "test/"
  ]
}
